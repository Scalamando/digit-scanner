		"jimp": "^0.16.1",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"tsoa": "^3.14.0",
		"typeorm": "^0.2.39"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/fabric": "^4.5.4",
		"@types/node": "^16.11.7",
		"@types/shelljs": "^0.8.9",
